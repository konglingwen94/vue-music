{
    "presets": [
        ["env",
        {
            "modules": false
        }],
        "stage-2"
    ],
    "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        ["component", [
        {
            "libraryName": "mint-ui",
            "style": true
        }]]
    ],
    "env":
    {
        "test":
        {
            "presets": ["env", "stage-2"],
            "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
        }
    }
}